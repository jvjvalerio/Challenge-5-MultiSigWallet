an\",{style:{marginRight:8},children:\"\\uD83D\\uDCDD\"}),\"Create your \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"b\",{children:\"local subgraph\"}),\" by running\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"span\",{className:\"highlight\",style:highlight,children:\"yarn graph-create-local\"}),\"(only required once!)\"]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{style:{margin:32},children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"span\",{style:{marginRight:8},children:\"\\uD83D\\uDEA2\"}),\"Deploy your \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"b\",{children:\"local subgraph\"}),\" by running\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"span\",{className:\"highlight\",style:highlight,children:\"yarn graph-ship-local\"})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{style:{margin:32},children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"span\",{style:{marginRight:8},children:\"\\uD83D\\uDD8D\\uFE0F\"}),\"Edit your \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"b\",{children:\"local subgraph\"}),\" in\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"span\",{className:\"highlight\",style:highlight,children:\"packages/subgraph/src\"}),\"(learn more about subgraph definition\",\" \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"a\",{href:\"https://thegraph.com/docs/define-a-subgraph\",target:\"_blank\",rel:\"noopener noreferrer\",children:\"here\"}),\")\"]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{style:{margin:32},children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"span\",{style:{marginRight:8},children:\"\\uD83E\\uDD29\"}),\"Deploy your \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"b\",{children:\"contracts and your subgraph\"}),\" in one go by running\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"span\",{className:\"highlight\",style:highlight,children:\"yarn deploy-and-graph\"})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{style:{width:780,margin:\"auto\",paddingBottom:64},children:[/*#__PURE__*/Object(jsx_runtime